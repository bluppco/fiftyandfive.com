---

// IMPORTS ASTRO COMPONENTS
import Footer from "@/components/common/footer/index.astro"
import Header from "@/components/common/header/index.astro"
import Hero from "@/components/home/hero/index.astro"
import Work from "@/components/home/work/index.astro"

// GET ASTRO PROPS
const {

    CDN,
    ENVIRONMENT,
    home_data,
    PROJECT

} = Astro.props

---
<Header
    ENVIRONMENT={ ENVIRONMENT }
/>
<Hero
    data={ home_data.hero }
/>
<Work
    CDN={ CDN }
    data={ home_data.work }
    PROJECT={ PROJECT }
/>
<Footer />
