---

// RUNTIME
const runtime = Astro.locals.runtime

const {

	ENDPOINT,
	ENVIRONMENT

} = runtime.env
const data = await contactAPI( ENVIRONMENT, ENDPOINT )

// API
import { contactAPI } from "@/api/contact"

const {

	contact

} = data

// IMPORTS LAYOUT COMPONENTS
import Layout from "@/layouts/layout/index.astro"

// ABOUT COMPONENT
import Contact from "@/components/contact/index.astro"

---
<Layout
	description=""
	image=""
	title=""
>
	<Contact
		data={ contact }
        ENVIRONMENT={ ENVIRONMENT }
    />
</Layout>
