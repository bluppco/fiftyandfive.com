---

// RUNTIME
const runtime = Astro.locals.runtime

const { ENVIRONMENT } = runtime.env

// IMPORTS ASTRO LAYOUTS
import Layout from "@/layouts/layout/index.astro"
import Container from "@/layouts/container/index.astro"

// IMPORTS ASTRO COMPONENTS
import Footer from "@/components/common/footer/index.astro"
import Header from "@/components/common/header/index.astro"
import HeaderBanner from "@/components/common/header-banner/index.astro"

// IMPORTS JSX COMPONENTS
import BenchmarkingJSX from "@/components/benchmarking/index.jsx"

// IMPORTS BENCHMARKING FROM MD
import { frontmatter as about_page_benchmarking } from "@/content/benchmarking/home.mdx"
import { frontmatter as contact_page_benchmarking } from "@/content/benchmarking/home.mdx"
import { frontmatter as home_page_benchmarking } from "@/content/benchmarking/home.mdx"
import { frontmatter as spotlight_page_benchmarking } from "@/content/benchmarking/home.mdx"

---
<Layout
	title={ "Benchmarking" }
	description={ "Benchmarking" }
	image=""
>
    <HeaderBanner
        ENVIRONMENT={ ENVIRONMENT }
    />
    <Header />
    <section class="py-40">
        <Container>
            <section class="relative h-full z-10 bg-white">
                <BenchmarkingJSX
                    about_page_benchmarking={ about_page_benchmarking }
                    client:load
                    contact_page_benchmarking={ contact_page_benchmarking }
                    home_page_benchmarking={ home_page_benchmarking }
                    spotlight_page_benchmarking={ spotlight_page_benchmarking }
                />
            </section>
        </Container>
    </section>
    <Footer />
</Layout>
